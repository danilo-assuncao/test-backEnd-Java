<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
	version="2.2">

	<!-- Regras Navegacao -->
	<navigation-rule>
	    <from-view-id>*</from-view-id>
	    <navigation-case>
	        <from-outcome>PLAYER_REGISTER</from-outcome>
	        <to-view-id>/pages/player/player_register.xhtml</to-view-id>
	    </navigation-case>
	</navigation-rule>
	
	<!-- Player -->
	<navigation-rule>
	    <from-view-id>/pages/player/player_register.xhtml</from-view-id>
	    <navigation-case>
	        <from-outcome>PLAYER_LIST</from-outcome>
	        <to-view-id>/pages/player/player_list.xhtml</to-view-id>
	    </navigation-case>
	</navigation-rule>
</faces-config>